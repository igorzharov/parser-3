<script type="text/javascript">
        (function ($, api) {
            var root = $('#00000000373ec493000000007835503b');
            var selectors = {
                'phone': '[data-type=phone]',
                'menu': '[data-type=menu]'
            };

            $(document).ready(function () {
                root.find(selectors.phone).each(function () {
                    var self = $(this);
                    var menu = self.find(selectors.menu);

                    self.on('mouseover', function () {
                        menu.attr('data-active', 'true');
                        menu.stop().show(300);
                    }).on('mouseout', function () {
                        menu.attr('data-active', 'false');
                        menu.stop().hide(300);
                    });
                });
            });
        })(jQuery, intec);

                    function fix_header(positionActive){
                var currentPosition = $(window).scrollTop();
                var headerDOM = $('#00000000373ec493000000007835503b .header-fixed');
                if (currentPosition > positionActive && !headerDOM.hasClass('menu-fixed')) {
                    headerDOM.addClass('menu-fixed');
                    headerDOM.animate({opacity: 1, top: 0}, 200);
                } else if (currentPosition <= positionActive && headerDOM.hasClass('menu-fixed')) {
                    headerDOM.removeClass('menu-fixed');
                    headerDOM.animate({opacity: 0, top: '-196px'}, 100);
                }
            }

            $(document).ready(function() {
                var menu = $('#00000000373ec493000000007835503b');
                var positionActive = menu.offset().top + $('.header-desktop', menu).outerHeight();
                $(window).scroll(function() {
                    fix_header(positionActive);
                });
            });
        
                    function fix_header_mobile(positionActive){
                var currentPosition = $(window).scrollTop();
                var headerDOM = $('.header-mobile');
                if (currentPosition > positionActive && !headerDOM.hasClass('header-fixed-menu')) {
                    headerDOM.addClass('header-fixed-menu');
                } else if (currentPosition <= positionActive && headerDOM.hasClass('header-fixed-menu')) {
                    headerDOM.removeClass('header-fixed-menu');
                }
            }

            $(document).ready(function() {
                var header = $('#00000000373ec493000000007835503b');
                var positionActive = header.offset().top + $('.header-mobile', header).outerHeight();
                $(window).scroll(function() {
                    fix_header_mobile(positionActive);
                });
            });
            </script>
