        try{document.form_auth.USER_LOGIN.focus();}catch(e){}
    </script><script type="text/javascript">
                (function ($, api) {
                    var root = $('#000000004887908a0000000056916887');
                    var buttons = {
                        'login': root.find('[data-action=login]')
                    };

                    buttons.login.on('click', function () {
                        var popup = new BX.PopupWindow(
                            'authPopup',
                            null,
                            {
                                content: $('#000000004887908a0000000056916887 .auth_popup_container').clone().get(0),
                                title: 'Авторизация',
                                closeIcon: {
                                    right: '20px',
                                    top: '22px'
                                },
                                zIndex: 0,
                                offsetLeft: 0,
                                offsetTop: 0,
                                width: 800,
                                overlay: true,
                                titleBar: {
                                    content: BX.create('span', {
                                        html: 'Авторизация',
                                        props: {
                                            className: 'access-title-bar'
                                        }
                                    })
                                }
                            });
                        popup.show();
                    });
                })(jQuery, intec);
